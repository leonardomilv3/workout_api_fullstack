generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CT {
  id String @id @default(uuid())
  nome String @unique
  endereco String
  proprietario String
  createdAt DateTime @default(now())
  createdBy String?
}

model Atleta {
  id String @id @default(uuid())
  cpf String @unique
}

model Categoria {
  id String @id @default(uuid())
  nome String @unique
}



